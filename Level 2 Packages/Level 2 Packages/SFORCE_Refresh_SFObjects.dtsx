			'Dts.Events.FireError("60000", "Notification-Eventhandler", "Package ID has been changed or notification framework has been corrupted. Please re-apply notification using BI xPress User Interface.", "", 0)
			'End If
			Return False
		Else
			Return True
		End If
	End Function

	' NOTE : Do not use double quotes {"} and semicolon {;} in the value field. E.g. MAX_NOTIFICATIONS=3;PRIORITY_MODE=URGENT;SMTP_UID=emailuser;SMTP_PWD=xx;@@4e; 
	' In above example SMTP_PWD=xx;@@4e this will cause problem in parsing because password is using {;}. System will consider password xx instead of xx;@@4e. 
	Public Function GetRuntimeSettings() As System.Collections.Hashtable
		Dim oRuntimeSettings As System.Collections.Hashtable = Nothing
		Dim sOtherConfigSettings As String = ReadVariable("User::varSSISOpsNF_OtherConfigSettings").ToString()

		Dim oVar As Object = ReadVariable("User::varSSISOpsNF_RuntimeSettings")

		If oVar Is Nothing Then
			oRuntimeSettings = New System.Collections.Hashtable()
		ElseIf oVar.ToString() = "System.Collections.Hashtable" Then
			Return DirectCast(oVar, System.Collections.Hashtable)
		Else
			oRuntimeSettings = New System.Collections.Hashtable()
		End If

		If String.IsNullOrEmpty(sOtherConfigSettings) = False Then
			Dim arrNameValue As String() = Nothing
			Dim arrTemp As String() = Nothing
			Dim sAttrName As String = Nothing
			Dim sAttrValue As String = Nothing

			Const PAIR_SEP As String = ";"
			Const NAMEVAL_SEP As String = "="

			arrNameValue = sOtherConfigSettings.Split(PAIR_SEP.ToCharArray())

			'Set Default values 
			oRuntimeSettings.Add("MAX_NOTIFICATIONS", CInt(1))
			oRuntimeSettings.Add("ENABLE_ROWCOUNT_THRESHOLD", CBool(False))
			oRuntimeSettings.Add("ENABLE_TIME_THRESHOLD", CBool(False))
			oRuntimeSettings.Add("PRIORITY_MODE", "DELAYED")
			oRuntimeSettings.Add("EMAIL_FORMAT", "HTML")
			oRuntimeSettings.Add("SMTP_PORT", CInt(25))
			oRuntimeSettings.Add("SMTP_USESSL", CBool(False))
			oRuntimeSettings.Add("SMTP_UID", "")
			oRuntimeSettings.Add("SMTP_PWD", "")
			oRuntimeSettings.Add("SMS_EMAILLIST", "")
			oRuntimeSettings.Add("FILTERED_ALERTS", "")
			oRuntimeSettings.Add("NOTIFY_ONERROR", CBool(True))
			oRuntimeSettings.Add("NOTIFY_ONWARNING", CBool(False))
			oRuntimeSettings.Add("NOTIFY_ONSUCCESS", CBool(False))
			oRuntimeSettings.Add("NOTIFY_ONCOMPLETE", CBool(False))
			oRuntimeSettings.Add("SMS_MAXLENGTH", CInt(160))
			oRuntimeSettings.Add("CC_EMAILLIST", "")
			oRuntimeSettings.Add("BCC_EMAILLIST", "")
			oRuntimeSettings.Add("NOTIFY_USING_EMAIL", CBool(True))
			oRuntimeSettings.Add("NOTIFY_USING_SMS", CBool(True))
			oRuntimeSettings.Add("NOTIFY_USING_EVENTLOG", CBool(False))
			oRuntimeSettings.Add("NOTIFY_USING_LOGFILE", CBool(False))
			oRuntimeSettings.Add("LOG_FOLDER_PATH", "C:\SSIS\Log")
			oRuntimeSettings.Add("DELETE_LOG_OLDER_THAN_DAYS", CInt(5))
			oRuntimeSettings.Add("USE_CUSTOM_EMAIL_TEMPLATE", CBool(False))
			oRuntimeSettings.Add("CUSTOM_EMAIL_TEMPLATE_FILEPATH", "C:\SSIS\Config\NF_EmailTemplate.htm")

			'ALERT_LIST 
			'ALERT_LIST_ALL 
			'NOTIFICATION_COUNTER 

			For Each sPair As String In arrNameValue
				arrTemp = sPair.Split(NAMEVAL_SEP.ToCharArray(), 2, StringSplitOptions.RemoveEmptyEntries)
				Try
					If arrTemp.Length >= 2 Then
						sAttrName = arrTemp(0)
						sAttrValue = arrTemp(1)
						Select Case sAttrName.ToUpper().Trim()
							Case "MAX_NOTIFICATIONS"
								oRuntimeSettings("MAX_NOTIFICATIONS") = Convert.ToInt32(sAttrValue)
								Exit Select
							Case "ENABLE_ROWCOUNT_THRESHOLD"
								oRuntimeSettings("ENABLE_ROWCOUNT_THRESHOLD") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "ENABLE_TIME_THRESHOLD"
								oRuntimeSettings("ENABLE_TIME_THRESHOLD") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "PRIORITY_MODE"
								' DELAYED (After Package) or URGENT (After Task) 
								oRuntimeSettings("PRIORITY_MODE") = DirectCast(sAttrValue, String)
								Exit Select
							Case "EMAIL_FORMAT"
								' TEXT or HTML 
								oRuntimeSettings("EMAIL_FORMAT") = DirectCast(sAttrValue, String)
								Exit Select
							Case "SMTP_PORT"
								' Default 25 
								oRuntimeSettings("SMTP_PORT") = Convert.ToInt32(sAttrValue)
								Exit Select
							Case "SMTP_USESSL"
								' Default False
								oRuntimeSettings("SMTP_USESSL") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "SMTP_UID"
								oRuntimeSettings("SMTP_UID") = DirectCast(sAttrValue, String)
								Exit Select
							Case "SMTP_PWD"
								oRuntimeSettings("SMTP_PWD") = DirectCast(sAttrValue, String)
								Exit Select
							Case "SMS_EMAILLIST"
								' Must be pipe seperated e.g. 66633422@tmobile.net|11223445@tmobile.net .... 
								oRuntimeSettings("SMS_EMAILLIST") = DirectCast(sAttrValue, String)
								Exit Select
							Case "FILTERED_ALERTS"
								' Must be pipe seperated e.g. -111|-222 .... 
								oRuntimeSettings("FILTERED_ALERTS") = DirectCast(sAttrValue, String)
								Exit Select
							Case "NOTIFY_ONERROR"
								oRuntimeSettings("NOTIFY_ONERROR") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_ONWARNING"
								oRuntimeSettings("NOTIFY_ONWARNING") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_ONSUCCESS"
								oRuntimeSettings("NOTIFY_ONSUCCESS") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_ONCOMPLETE"
								oRuntimeSettings("NOTIFY_ONCOMPLETE") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "SMS_MAXLENGTH"
								oRuntimeSettings("SMS_MAXLENGTH") = Convert.ToInt32(sAttrValue)
								Exit Select
							Case "CC_EMAILLIST"
								oRuntimeSettings("CC_EMAILLIST") = DirectCast(sAttrValue, String)
								Exit Select
							Case "BCC_EMAILLIST"
								oRuntimeSettings("BCC_EMAILLIST") = DirectCast(sAttrValue, String)
								Exit Select
							Case "NOTIFY_USING_EMAIL"
								oRuntimeSettings("NOTIFY_USING_EMAIL") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_USING_SMS"
								oRuntimeSettings("NOTIFY_USING_SMS") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_USING_EVENTLOG"
								oRuntimeSettings("NOTIFY_USING_EVENTLOG") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "NOTIFY_USING_LOGFILE"
								oRuntimeSettings("NOTIFY_USING_LOGFILE") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "LOG_FOLDER_PATH"
								oRuntimeSettings("LOG_FOLDER_PATH") = DirectCast(sAttrValue, String)
								Exit Select
							Case "DELETE_LOG_OLDER_THAN_DAYS"
								oRuntimeSettings("DELETE_LOG_OLDER_THAN_DAYS") = Convert.ToInt32(sAttrValue)
								Exit Select
							Case "USE_CUSTOM_EMAIL_TEMPLATE"
								oRuntimeSettings("USE_CUSTOM_EMAIL_TEMPLATE") = Convert.ToBoolean(sAttrValue)
								Exit Select
							Case "CUSTOM_EMAIL_TEMPLATE_FILEPATH"
								oRuntimeSettings("CUSTOM_EMAIL_TEMPLATE_FILEPATH") = DirectCast(sAttrValue, String)
								Exit Select
						End Select
					End If
				Catch ex As Exception
					Dts.Events.FireError(0, "NFEventHandler", ex.Message, "", 0)
				End Try
			Next
		End If

		oRuntimeSettings.Add("ALERT_LIST", New System.Collections.ArrayList())
		oRuntimeSettings.Add("ALERT_LIST_ALL", New System.Collections.ArrayList())
		oRuntimeSettings.Add("NOTIFICATION_COUNTER", CInt(0))

		SetNF(oRuntimeSettings)

		WriteVariable("User::varSSISOpsNF_RuntimeSettings", oRuntimeSettings)

		Return oRuntimeSettings
	End Function

	Sub SetNF(oRuntimeSettings As HashTable)
		oRuntimeSettings.Add(Chr(78) & Chr(70) & Chr(95) & Chr(67) & Chr(72) & Chr(69) & Chr(67) & Chr(75), ReadVariable("User::varSSISOpsNF_" & Chr(67) & Chr(72) & Chr(69) & Chr(67) & Chr(75)))
	End Sub

