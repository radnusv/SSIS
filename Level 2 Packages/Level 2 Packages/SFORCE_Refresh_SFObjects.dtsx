	Private Sub UpdateAlertList()
		Dim sErrorDescription, sSourceName, sSourceID As String
		Dim iErrorCode As String
		Dim oAlertList As System.Collections.ArrayList
		Dim oRuntimeSettings As System.Collections.Hashtable
		Dim oAlertInfo As AlertInfo
		Dim sFilteredAlerts As String

		oRuntimeSettings = GetRuntimeSettings()

		CheckNF(oRuntimeSettings)

		sFilteredAlerts = "|" & oRuntimeSettings("FILTERED_ALERTS").ToString() & "|"

		iErrorCode = ReadVariable("System::ErrorCode").ToString()

		If sFilteredAlerts.IndexOf("|" & iErrorCode.ToString() & "|") >= 0 Then
			' Filter This
			Exit Sub
		End If

		sErrorDescription = ReadVariable("System::ErrorDescription").ToString()
		sSourceName = ReadVariable("System::SourceName").ToString()
		sSourceID = ReadVariable("System::SourceID").ToString()

		Dim bIsErrorScope As Boolean

		Try
			If Dts.VariableDispenser.Contains("System::Cancel") Then
				bIsErrorScope = True
			Else
				bIsErrorScope = False
			End If
		Catch ex As Exception
			bIsErrorScope = False
		End Try

		Dim iAlertType As enumAlertType

		If bIsErrorScope Then
			iAlertType = enumAlertType.ErrorAlert
		Else
			iAlertType = enumAlertType.WarningAlert
		End If

		oAlertInfo = New AlertInfo(sErrorDescription, CInt(iErrorCode), sSourceName, sSourceID, iAlertType)
		oAlertList = CType(oRuntimeSettings("ALERT_LIST"), ArrayList)
		oAlertList.Add(oAlertInfo)
		oAlertList = CType(oRuntimeSettings("ALERT_LIST_ALL"), ArrayList)
		oAlertList.Add(oAlertInfo)
	End Sub

