 Imports System
 Imports System.Data
 Imports System.Math
 Imports Microsoft.SqlServer.Dts.Runtime
 <System.AddIn.AddIn("ScriptMain", Version:="1.0", Publisher:="", Description:="")> _ 
 <System.CLSCompliantAttribute(False)> _
 Partial Public Class ScriptMain
      Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
      Enum ScriptResults
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
      End Enum
      Public Sub Main()
          'TODO: Add code below
        Try
        Dim var As String = ReadVariable("System::VariableName").ToString()
        Dim filterList as String = ReadVariable("User::varSSISOps_FilteredVars").ToString()
        Dim arrFilter() as String = filterList.Split(",".ToCharArray(), StringSplitOptions.RemoveEmptyEntries)

               Dim bSkip As Boolean = False
               Dim comp As String = var
               For Each f As String In arrFilter
                   comp = comp.ToLower().Trim()
                   f = f.ToLower().Trim()
                   If String.IsNullOrEmpty(f) Then
                       Continue For
                   End If
                   If comp.IndexOf("::") < 0 Then comp = "user::" & comp
                   If f.IndexOf("::") < 0 Then f = "user::" & f
                   If System.Text.RegularExpressions.Regex.IsMatch(comp,f.Replace("*","\w*"),Text.RegularExpressions.RegexOptions.IgnoreCase) Then
                       bSkip = True
                       Exit For
                   End If
               Next

            If bSkip Then
                WriteVariable("User::varSSISOps_CurValue", "*****")
                Exit Sub
            End If
            Dim val As Object = ReadVariableEx("System::VariableValue")
            If (Not (val Is DBNull.Value)) AndAlso val IsNot Nothing Then
                Dim s As String = val.ToString()
                Dim iMaxCharsToLog As Integer = Convert.ToInt32(ReadVariable("User::varSSISOps_MaxVarCharsToLog"))
                Dim bLogFullValue As Boolean = IIF(iMaxCharsToLog<=0, True, False)
                If bLogFullValue OrElse s.Length <= iMaxCharsToLog Then
                    WriteVariable("User::varSSISOps_CurValue", s)
                Else
                    WriteVariable("User::varSSISOps_CurValue", s.Substring(0, iMaxCharsToLog) & " ...<VALUE TRUNCATED - Change value of [varSSISOps_MaxVarCharsToLog] to -1 from config file if you want to log full value (Not recommended for performance reason)>")
                End If
            Else
                WriteVariable("User::varSSISOps_CurValue", "")
            End If
        Catch ex As Exception
            Dts.Events.FireWarning(0, "VariableValueChanged", "Can not log value for [" & ReadVariable("System::VariableName").ToString() & "]", "", 0)
        End Try


          Dts.TaskResult = ScriptResults.Success
      End Sub
    Private Function ReadVariableEx(ByVal varName As String) As Object
        Dim val As Object = ReadVariable(varName)

        Dim bNull As Boolean
        If DBNull.Value.Equals(val) OrElse val Is Nothing Then
            bNull = True
        End If

        If Not bNull Then
            Dim s As String = val.GetType.ToString()

            '//Check if variable is ADO Recordset
            If s = "System.__ComObject" AndAlso TypeName(val) = "Recordset" Then
                Dim oleDA As New OleDb.OleDbDataAdapter
                Dim dt As New DataTable
                oleDA.Fill(dt, val)
                dt.TableName = "AdoRecordset"
                Dim tab As String = vbTab '//Tab character
                Dim newline As String = Environment.NewLine 'New line character

                Dim sbVal As New System.Text.StringBuilder(1000)
                Dim colCnt As Integer = dt.Columns.Count
                Dim rowCnt As Integer = dt.Rows.Count
                Dim iRowCnt, iColCnt As Integer

                '//Column names
                Dim sLine As String = ""
                Dim lineChar As String = "="
                sbVal.AppendLine("###ADO Recordset (" & dt.Rows.Count & " Records)###... [See Detail]")
                sbVal.Append("Row#" & tab)
                sLine = sLine & lineChar.PadLeft(10).Replace(" ", lineChar) & tab
                For Each col As DataColumn In dt.Columns
                    sbVal.Append(col.Caption & tab)
                    sLine = sLine & lineChar.PadLeft(10).Replace(" ", lineChar) & tab
                Next
                sbVal.Append(newline & sLine & newline)

                For Each row As DataRow In dt.Rows
                    iColCnt = 0
                    sbVal.Append("[" & iRowCnt+1 & "]" & tab)
                    For iColCnt = 0 To colCnt - 1
                        If iColCnt = colCnt - 1 Then
                            sbVal.Append(GetStrDBVal(row(iColCnt)))
                        Else
                            sbVal.Append(GetStrDBVal(row(iColCnt)) & tab)
                        End If
                    Next
                    If iRowCnt < rowCnt - 1 Then
                        sbVal.Append(newline)
                    End If
                    iRowCnt=iRowCnt+1
                Next

                Return sbVal.ToString()
            Else
                Return val '//Its not ADO Recordset
            End If
        Else
            Return val '//Its null
        End If
    End Function
    Public Function GetStrDBVal(ByVal obj As Object) As String
        If (obj Is System.DBNull.Value) Then
            Return "<null>"
        ElseIf (obj Is Nothing) Then
            Return ""
        Else
            Return obj.ToString()
        End If
    End Function

  Private Sub WriteVariable(ByVal varName As String, ByVal varValue As Object)
      Try
          Dim vars As Variables
          Dts.VariableDispenser.LockForWrite(varName)
          Dts.VariableDispenser.GetVariables(vars)
          Try
              vars(varName).Value = varValue
          Catch ex As Exception
              Throw ex
          Finally
              vars.Unlock()
          End Try
      Catch ex As Exception
          Throw ex
      End Try
  End Sub
  Private Function ReadVariable(ByVal varName As String) As Object
      Dim result As Object
      Try
          Dim vars As Variables
          Dts.VariableDispenser.LockForRead(varName)
          Dts.VariableDispenser.GetVariables(vars)
          Try
              result = vars(varName).Value
          Catch ex As Exception
              Throw ex
          Finally
              vars.Unlock()
          End Try
      Catch ex As Exception
          Throw ex
      End Try
      Return result
  End Function
 End Class]]></ProjectItem><ProjectItem Name="\my project\resources.resx">
