    Private Sub PerformNotificationAction()
        Const PREFIX_DISABLED As String = "{D}"

        Dim oMessage As MailMessage = Nothing
        Dim oMessageSMS As MailMessage = Nothing
        Dim oSmtpClient As SmtpClient = Nothing
        Dim oAlertList As System.Collections.ArrayList = Nothing
        Dim oRuntimeSettings As System.Collections.Hashtable = Nothing
        Dim bPkgRunning As Boolean = False
        Dim sPkgName As String = ReadVariable("System::PackageName").ToString()
        Dim dtStart As System.DateTime = DirectCast(ReadVariable("System::StartTime"), DateTime)
        Dim dtEnd As System.DateTime = DateTime.Now
        Dim sExecAcct As String = ReadVariable("System::UserName").ToString()
        Dim sMachine As String = ReadVariable("System::MachineName").ToString()
        Dim sInteractive As String = ReadVariable("System::InteractiveMode").ToString()
        Dim sStepName As String = Nothing

        oRuntimeSettings = GetRuntimeSettings()
        If CheckNF(oRuntimeSettings) = False Then
        End If
        '''/Exit Sub : PackageID Changed 

        If ReadVariable("System::SourceID").ToString() = ReadVariable("System::PackageID").ToString() Then
            '''/Package finished running 
            bPkgRunning = False
            sStepName = ""
            oAlertList = DirectCast(GetSetting("ALERT_LIST_ALL", oRuntimeSettings), ArrayList)
        Else
            bPkgRunning = True
            '''/Task finished running 
            If DirectCast(GetSetting("PRIORITY_MODE", oRuntimeSettings), String) = "DELAYED" Then
                '''/Do all work when package is completed 
                Exit Sub
            End If
            'sStepName = ReadVariable("System::SourceName").ToString(); 
            oAlertList = DirectCast(GetSetting("ALERT_LIST", oRuntimeSettings), ArrayList)
            sStepName = DirectCast(GetLastAlertSource(oAlertList), String)
        End If

        Dim sBody As String = Nothing
        Dim sSubjectLine As String = Nothing
        Dim sBodySMS As String = Nothing
        Dim iErrors As Integer = 0
        Dim iWarnings As Integer = 0
        Dim iOther As Integer = 0
        Dim iCurNotifyCount As Integer = DirectCast(GetSetting("NOTIFICATION_COUNTER", oRuntimeSettings), Int32)
        Dim iMaxNotifyCount As Integer = DirectCast(GetSetting("MAX_NOTIFICATIONS", oRuntimeSettings), Int32)

        Dim bEmailMethod As Boolean = CBool(GetSetting("NOTIFY_USING_EMAIL", oRuntimeSettings))
        Dim bSMSMethod As Boolean = CBool(GetSetting("NOTIFY_USING_SMS", oRuntimeSettings))
        Dim bEventLogMethod As Boolean = CBool(GetSetting("NOTIFY_USING_EVENTLOG", oRuntimeSettings))
        Dim bLogFileMethod As Boolean = CBool(GetSetting("NOTIFY_USING_LOGFILE", oRuntimeSettings))
        Dim bUseCustEmailTemplate As Boolean = CBool(GetSetting("USE_CUSTOM_EMAIL_TEMPLATE", oRuntimeSettings))
        Dim sCustEmailTemplatePath As String = DirectCast(GetSetting("CUSTOM_EMAIL_TEMPLATE_FILEPATH", oRuntimeSettings), String)
        Dim sLogFolderPath As String = DirectCast(GetSetting("LOG_FOLDER_PATH", oRuntimeSettings), String)
        Dim iDeleteLogOlderThanDays As Integer = CInt(GetSetting("DELETE_LOG_OLDER_THAN_DAYS", oRuntimeSettings))

        If oAlertList.Count <= 0 AndAlso (CBool(GetSetting("NOTIFY_ONCOMPLETE", oRuntimeSettings)) = False AndAlso CBool(GetSetting("NOTIFY_ONSUCCESS", oRuntimeSettings)) = False) Then
            Exit Sub
        End If
        If oAlertList.Count <= 0 AndAlso bPkgRunning = True Then
            Exit Sub
        End If
        If bPkgRunning = True AndAlso iCurNotifyCount >= iMaxNotifyCount AndAlso CBool(GetSetting("NOTIFY_ONCOMPLETE", oRuntimeSettings)) = False Then
            Exit Sub
        End If

        Dim bDelayedMode As Boolean = False
        If DirectCast(GetSetting("PRIORITY_MODE", oRuntimeSettings), String) = "DELAYED" Then
            bDelayedMode = True
        End If
        If CBool(GetSetting("NOTIFY_ONCOMPLETE", oRuntimeSettings)) = False AndAlso bDelayedMode = False AndAlso iCurNotifyCount > 0 Then
            Exit Sub
        End If

        '''/Send Email 
        Dim sFrom As String = ReadVariable("User::varSSISOpsNF_From").ToString()
        Dim sEmailList As String = ""
        If bEmailMethod = True AndAlso ((ReadVariable("User::varSSISOpsNF_To") IsNot Nothing)) Then
            sEmailList = ReadVariable("User::varSSISOpsNF_To").ToString()
            sEmailList = GetFilteredEmailList(sEmailList, ";", PREFIX_DISABLED)
        End If

        If Not String.IsNullOrEmpty(sEmailList) Then
            oMessage = New MailMessage()
            oMessage.From = New MailAddress(sFrom)
            oMessage.[To].Add(sEmailList.Replace(";", ","))
            If bDelayedMode=False Then
				oMessage.Priority = MailPriority.High
            End If

            Dim sCCList As String = DirectCast(GetSetting("CC_EMAILLIST", oRuntimeSettings), String)
            Dim sBCCList As String = DirectCast(GetSetting("BCC_EMAILLIST", oRuntimeSettings), String)

            If String.IsNullOrEmpty(sCCList) = False Then
                oMessage.CC.Add(sCCList.Replace(";", ",").Replace("|", ","))
            End If
            If String.IsNullOrEmpty(sBCCList) = False Then
                oMessage.Bcc.Add(sBCCList.Replace(";", ",").Replace("|", ","))
            End If

            If DirectCast(GetSetting("EMAIL_FORMAT", oRuntimeSettings), String) = "HTML" Then
                oMessage.IsBodyHtml = True
                sBody = GetHTMLNotificationMessage(sPkgName, dtStart, dtEnd, FormatTimeDifference(dtStart, dtEnd), sMachine, sExecAcct, _
                sInteractive, bPkgRunning, sStepName, iErrors, iWarnings, iOther, _
                DirectCast(oAlertList, ArrayList), False, 0, bUseCustEmailTemplate, sCustEmailTemplatePath)
            Else
                oMessage.IsBodyHtml = False
                sBody = GetTextNotificationMessage(sPkgName, dtStart, dtEnd, FormatTimeDifference(dtStart, dtEnd), sMachine, sExecAcct, _
                sInteractive, bPkgRunning, sStepName, iErrors, iWarnings, iOther, _
                DirectCast(oAlertList, ArrayList), False, 0, bUseCustEmailTemplate, sCustEmailTemplatePath)
            End If
            oMessage.Body = sBody
        End If

        If (bEventLogMethod = True) OrElse (bLogFileMethod = True) Then
			'Dim iErrors_tmp, iWarnings_tmp, iOther_tmp As Integer
			iErrors = 0
			iWarnings = 0
			iOther = 0
            sBody = GetTextNotificationMessage(sPkgName, dtStart, dtEnd, FormatTimeDifference(dtStart, dtEnd), sMachine, sExecAcct, _
            sInteractive, bPkgRunning, sStepName, iErrors, iWarnings, iOther, _
            DirectCast(oAlertList, ArrayList), False, 0, False, "")
            
            'If iErrors_tmp > 0 Then iErrors = iErrors_tmp
            'If iWarnings_tmp > 0 Then iWarnings = iWarnings_tmp
            'If iOther_tmp > 0 Then iOther = iOther_tmp
             
            If (bLogFileMethod = True) AndAlso (iDeleteLogOlderThanDays > 0) Then
                DeleteOldFiles(sLogFolderPath, "NF_LOG_*.txt", iDeleteLogOlderThanDays)
            End If
        End If

        If (iWarnings > 0 AndAlso CBool(GetSetting("NOTIFY_ONWARNING", oRuntimeSettings)) = True) Then
        Else
            '''/Yes Send Alert 
            If (iErrors > 0 AndAlso CBool(GetSetting("NOTIFY_ONSUCCESS", oRuntimeSettings)) = True AndAlso CBool(GetSetting("NOTIFY_ONCOMPLETE", oRuntimeSettings)) = False AndAlso CBool(GetSetting("NOTIFY_ONERROR", oRuntimeSettings)) = False AndAlso CBool(GetSetting("NOTIFY_ONWARNING", oRuntimeSettings)) = False) Then
                Exit Sub
            End If
        End If


        '''/Send SMS 
        Dim iMaxLen As Integer = CInt(GetSetting("SMS_MAXLENGTH", oRuntimeSettings))
        Dim sSMSList As String = DirectCast(GetSetting("SMS_EMAILLIST", oRuntimeSettings), String)
        If String.IsNullOrEmpty(sSMSList) = False Then
            sSMSList = GetFilteredEmailList(sSMSList, "|", PREFIX_DISABLED)
        End If

        If bSMSMethod = True AndAlso Not String.IsNullOrEmpty(sSMSList) Then
            oMessageSMS = New MailMessage()
            oMessageSMS.From = New MailAddress(sFrom)
            oMessageSMS.[To].Add(sSMSList.Replace("|", ","))
            iErrors = 0
            iWarnings = 0
            iOther = 0
            sBodySMS = GetTextNotificationMessage(sPkgName, dtStart, dtEnd, FormatTimeDifference(dtStart, dtEnd), sMachine, sExecAcct, _
            sInteractive, bPkgRunning, sStepName, iErrors, iWarnings, iOther, _
            DirectCast(oAlertList, ArrayList), True, iMaxLen, False, "")
            Dim sTempBody As String = Nothing
            sTempBody = ("<SSIS ERROR>" & System.Environment.NewLine) + sBodySMS
            If sTempBody.Length >= iMaxLen - sFrom.Length Then
                oMessageSMS.Body = sTempBody.Substring(iMaxLen - sFrom.Length)
            Else
                oMessageSMS.Body = sTempBody
            End If
        End If

        If bPkgRunning = True Then
            If iErrors > 0 Then
                sSubjectLine = ("ERROR : Package " & ReadVariable("System::PackageName") & " has error(s) at task [") & ReadVariable("System::SourceName") & "]"
            ElseIf iWarnings > 0 Then
                sSubjectLine = ("WARNING : Package " & ReadVariable("System::PackageName") & " has warning(s) at task [") & ReadVariable("System::SourceName") & "]"
            Else
                If iOther > 0 Then
                    sSubjectLine = "ALERT : Package " & ReadVariable("System::PackageName") & " generated runtime alert(s)"
                Else
                    Exit Sub
                End If
            End If
        Else
            If iErrors > 0 Then
                sSubjectLine = ("ERROR : Package " & ReadVariable("System::PackageName") & " completed with [") & iErrors & "] error(s)"
            ElseIf iWarnings > 0 Then
                sSubjectLine = ("WARNING : Package " & ReadVariable("System::PackageName") & " completed with [") & iWarnings & "] warning(s)"
            ElseIf iOther > 0 Then
                sSubjectLine = ("ALERT : Package " & ReadVariable("System::PackageName") & " completed with [") & iOther & "] alert(s)"
            Else
                sSubjectLine = "SUCCESS : Package " & ReadVariable("System::PackageName") & " completed successfully"
            End If
        End If

        If (bEmailMethod = True) OrElse (bSMSMethod = True) Then
            oSmtpClient = New SmtpClient(ReadVariable("User::varSSISOpsNF_SMTPServerName").ToString(), CInt(GetSetting("SMTP_PORT", oRuntimeSettings)))
            If String.IsNullOrEmpty(GetSetting("SMTP_UID", oRuntimeSettings).ToString()) = False Then
                oSmtpClient.Credentials = New System.Net.NetworkCredential(GetSetting("SMTP_UID", oRuntimeSettings).ToString(), GetSetting("SMTP_PWD", oRuntimeSettings).ToString())
            Else
				oSmtpClient.UseDefaultCredentials = True
            End If
        End If
        If (CBool(GetSetting("SMTP_USESSL", oRuntimeSettings))= True)
			oSmtpClient.EnableSsl = True
        End If
                    
        If bPkgRunning = True Then
            '''/Already sent immediate mode alert 
            oAlertList.Clear()
        End If
        If (oMessage IsNot Nothing) Then
            oMessage.Subject = sSubjectLine
            oSmtpClient.Send(oMessage)
        End If
        If (oMessageSMS IsNot Nothing) Then
            If iMaxLen > 400 Then
                '''/Must be MMS 
                oMessageSMS.Subject = sSubjectLine
                If bDelayedMode=False Then
					oMessageSMS.Priority = MailPriority.High
				End If                
            Else
                oMessageSMS.Subject = ""
            End If
            oSmtpClient.Send(oMessageSMS)
        End If

        '''/Event Log or File Log 
        If (bEventLogMethod = True) OrElse (bLogFileMethod = True) Then

            If bEventLogMethod = True Then
                If iErrors > 0 Then
                    WriteToEventLog(sBody, "BI xPress", System.Diagnostics.EventLogEntryType.[Error], "Application")
                ElseIf iWarnings > 0 Then
                    WriteToEventLog(sBody, "BI xPress", System.Diagnostics.EventLogEntryType.Warning, "Application")
                Else
                    WriteToEventLog(sBody, "BI xPress", System.Diagnostics.EventLogEntryType.Information, "Application")
                End If
            End If

            If bLogFileMethod = True Then
                'Log Event to Text File 
                Dim sPath As String = Nothing
                Dim sFileName As String = Nothing
                sPkgName = sPkgName.Replace("<", "").Replace(">", "").Replace(":", "").Replace("/", "").Replace("\", "").Replace("*", "").Replace("|", "").Replace("?", "")
                If iErrors > 0 Then
                    sFileName = ("NF_LOG_" & System.DateTime.Now.ToString("yyyy-MM-dd hhmmss") & "_ERROR_") + sPkgName & ".txt"
                ElseIf iWarnings > 0 Then
                    sFileName = ("NF_LOG_" & System.DateTime.Now.ToString("yyyy-MM-dd hhmmss") & "_WARNING_") + sPkgName & ".txt"
                Else
                    sFileName = ("NF_LOG_" & System.DateTime.Now.ToString("yyyy-MM-dd hhmmss") & "_INFO_") + sPkgName & ".txt"
                End If

                sPath = System.IO.Path.Combine(sLogFolderPath, sFileName)
                SaveTextToFile(sBody, sPath, False, Nothing)
            End If
        End If

        '''/Increment notification counter 

        SetSetting("NOTIFICATION_COUNTER", CInt(GetSetting("NOTIFICATION_COUNTER", oRuntimeSettings)) + 1, oRuntimeSettings)
    End Sub

    Public Function WriteToEventLog(ByVal Entry As String, ByVal AppName As String, ByVal EventType As System.Diagnostics.EventLogEntryType, ByVal LogName As String) As Boolean
        Dim objEventLog As New System.Diagnostics.EventLog()
        Try
            'Register the App as an Event Source 
            If Not System.Diagnostics.EventLog.SourceExists(AppName) Then
                System.Diagnostics.EventLog.CreateEventSource(AppName, LogName)
            End If
            objEventLog.Source = AppName
            objEventLog.WriteEntry(Entry, EventType)
            Return True
        Catch Ex As Exception
            Return False
        End Try
    End Function

    Public Function SaveTextToFile(ByVal strData As String, ByVal FullPath As String, ByVal Append As Boolean, ByVal Encoding As System.Text.Encoding) As Boolean

        Dim bAns As Boolean = False
        Dim objWriter As System.IO.StreamWriter = Nothing
        Try
            If Encoding Is Nothing Then
                objWriter = New System.IO.StreamWriter(FullPath, Append)
            Else
                objWriter = New System.IO.StreamWriter(FullPath, Append, Encoding)
            End If

            objWriter.Write(strData)
            objWriter.Close()
            bAns = True
        Catch ex As Exception
            Throw ex
        Finally
            If objWriter IsNot Nothing Then
                objWriter.Close()
            End If
        End Try
        Return bAns
    End Function
    '''/This will delete files with specified pattern which are older than specified days 
    Public Function DeleteOldFiles(ByVal sPath As String, ByVal sPattern As String, ByVal OlderThanDays As Integer) As Integer
        'string myfile = null; 
        Dim fi As System.IO.FileInfo = Nothing
        Dim files As String() = System.IO.Directory.GetFiles(sPath, sPattern)
        Dim iDeleted As Integer = 0
        Try

            For Each myfile As String In files
                fi = New System.IO.FileInfo(myfile)
                '''/write full path of this file 
                'Debug.Write(fi.FullName) 
                If fi.CreationTime < DateTime.Today.AddDays(0 - OlderThanDays) Then
                    fi.Delete()
                    iDeleted = iDeleted + 1
                End If
            Next
        Catch ex As Exception
            Throw ex
        End Try
        Return iDeleted
    End Function

    Public Function GetSubStringBetween(ByVal stringToParse As String, ByVal startString As String, ByVal endString As String) As String
        Dim subString As String = Nothing
        If String.IsNullOrEmpty(stringToParse) Then
            Return Nothing
        End If
        Dim startPosition As Integer = stringToParse.IndexOf(startString)
        Dim endPosition As Integer = stringToParse.IndexOf(endString)
        If startPosition < 0 Or endPosition < 0 Then
            Return Nothing
        End If
        subString = stringToParse.Substring(startPosition + startString.Length, endPosition - (startPosition + startString.Length))
        Return subString
    End Function


    '''/Example : ReplaceSubStringBetween("---<a>test</a>---", "<a>", "</a>", "HHH",False) ===> ---<a>HHH</a>--- 
    '''/Example : ReplaceSubStringBetween("---<a>test</a>---", "<a>", "</a>", "HHH",True) ===> ---HHH--- 
    Public Function ReplaceSubStringBetween(ByVal stringToParse As String, ByVal startString As String, ByVal endString As String, ByVal replacewith As String, ByVal replacemarkers As Boolean) As String
        'Dim subString As String 
        If String.IsNullOrEmpty(stringToParse) Then
            Return Nothing
        End If

        Dim startPosition As Integer = stringToParse.IndexOf(startString)
        Dim endPosition As Integer = stringToParse.IndexOf(endString)
        If startPosition < 0 Or endPosition < 0 Then
            Return stringToParse
        End If
        'subString = stringToParse.Substring(startPosition, endPosition - startPosition) 
        Dim s1 As String = Nothing
        Dim s2 As String = Nothing
        If replacemarkers = False Then
            s1 = stringToParse.Remove(startPosition + startString.Length, (endPosition) - (startPosition + startString.Length))
            s2 = s1.Insert(startPosition + startString.Length, replacewith)
        Else
            s1 = stringToParse.Remove(startPosition, (endPosition + endString.Length) - (startPosition))
            s2 = s1.Insert(startPosition, replacewith)
        End If

        Return s2
    End Function
    Private Function GetPkgStatus(ByVal bPkgRunning As Boolean, ByVal sStepName As String, ByVal iErrors As Integer, ByVal iWarnings As Integer) As String
        Dim sStatus As String = Nothing
        Dim s As String = ""

        If String.IsNullOrEmpty(sStepName) = False Then
            s = " @ STEP [" & sStepName & "]"
        End If

        If iErrors > 0 Then
            If bPkgRunning = False Then
                sStatus = "COMPLETED W/ ERROR(S)"
            Else
                sStatus = ("HAS " & (IIf(iErrors > 0, " ERROR(S)", ""))) & s
            End If
        Else
            If bPkgRunning = False Then
                sStatus = "COMPLETED" & (IIf(iWarnings > 0, " W/ WARNING(S)", ""))
            Else
                sStatus = ("HAS " & (IIf(iWarnings > 0, " WARNING(S)", ""))) & s
            End If
        End If

        Return sStatus
    End Function


    Private Function GetTextNotificationMessage(ByVal sPkgName As String, ByVal dtStartTime As DateTime, ByVal dtEndTime As DateTime, ByVal sRunTime As String, ByVal sMachineName As String, ByVal sExecAcctName As String, _
    ByVal sInteractive As String, ByVal bPkgRunning As Boolean, ByVal sStepName As String, ByRef iErrors As Integer, ByRef iWarnings As Integer, ByRef iOtherAlerts As Integer, _
    ByVal oAlertList As ArrayList, ByVal bForSMS As Boolean, ByVal iSMSLength As Integer, ByVal bUseCustEmailTemplate As Boolean, ByVal sCustEmailTemplatePath As String) As String

        Dim oSB As New System.Text.StringBuilder()
        Dim oSBAlerts As New System.Text.StringBuilder()

        Dim sbAlerts As New System.Text.StringBuilder()
        Dim sStatus As String = Nothing

        Dim oAlertInfo As Object = Nothing
        Dim sAlertType As String = Nothing

        Dim sTemplate As String = ""
        If bForSMS = True Then
            bUseCustEmailTemplate = False
        End If

        Dim sAlertRow As String = ""
        Dim sAlertRows As String = ""

        If bUseCustEmailTemplate = True Then
            Try
                sTemplate = System.IO.File.ReadAllText(sCustEmailTemplatePath)
                sAlertRow = GetSubStringBetween(sTemplate, "<!--{{AlertBlockStart}}-->", "<!--{{AlertBlockEnd}}-->")
            Catch ex As Exception
                bUseCustEmailTemplate = False
            End Try
        End If


        If oAlertList.Count > 0 Then
            If bForSMS = False Then
                oSBAlerts.AppendLine("************ Total [" & oAlertList.Count & "] Alerts *************")
            Else
                oSBAlerts.AppendLine("<< [" & oAlertList.Count & "] Alerts >> ")
            End If
            oSBAlerts.AppendLine("")

            For Each oLoopAlertInfo As Object In oAlertList
                If DirectCast(GetPropVal(oLoopAlertInfo, "AlertType"), enumAlertType) = enumAlertType.ErrorAlert Then
                    sAlertType = "ERROR"
                    iErrors = iErrors + 1
                ElseIf DirectCast(GetPropVal(oLoopAlertInfo, "AlertType"), enumAlertType) = enumAlertType.WarningAlert Then
                    sAlertType = "WARNING"
                    iWarnings = iWarnings + 1
                ElseIf DirectCast(GetPropVal(oLoopAlertInfo, "AlertType"), enumAlertType) = enumAlertType.TimeThresholdAlert Then
                    sAlertType = "TIME THRESHOLD ALERT"
                    iOtherAlerts = iOtherAlerts + 1
                Else
                    sAlertType = "ALERT"
                End If

                If bForSMS Then
                    oSBAlerts.AppendLine(((GetPropVal(oLoopAlertInfo, "SourceName").ToString() & " >> ") + sAlertType & " : ") + GetPropVal(oLoopAlertInfo, "AlertDescription").ToString())
                Else
                    If bUseCustEmailTemplate AndAlso String.IsNullOrEmpty(sAlertRow) = False Then
                        sAlertRows = sAlertRows + sAlertRow.Replace("{{AlertSourceName}}", GetPropVal(oLoopAlertInfo, "SourceName").ToString()).Replace("{{AlertTime}}", GetPropVal(oLoopAlertInfo, "AlertTimeStamp").ToString()).Replace("{{AlertType}}", sAlertType).Replace("{{AlertMessage}}", GetPropVal(oLoopAlertInfo, "AlertDescription").ToString()).Replace("{{AlertCode}}", GetPropVal(oLoopAlertInfo, "AlertCode").ToString())
                    Else
                        oSBAlerts.AppendLine((((("[" & DirectCast(GetPropVal(oLoopAlertInfo, "AlertTimeStamp"), DateTime).ToString("MM/dd HH:mm:ss") & "] ") + GetPropVal(oLoopAlertInfo, "SourceName").ToString() & " = > ") + sAlertType & "[") + GetPropVal(oLoopAlertInfo, "AlertCode").ToString() & "] : ") + GetPropVal(oLoopAlertInfo, "AlertDescription").ToString() + System.Environment.NewLine)
                    End If
                End If
            Next
        End If  

        sStatus = GetPkgStatus(bPkgRunning, sStepName, iErrors, iWarnings)

        Dim sShortStatus As String = GetShortStatus(iErrors, iWarnings)

        If bUseCustEmailTemplate Then

            sTemplate = sTemplate.Replace("{{PackageName}}", sPkgName).Replace("{{StatusLong}}", sStatus).Replace("{{StatusShort}}", sShortStatus).Replace("{{TotalAlertCount}}", (iErrors + iWarnings).ToString()).Replace("{{ErrorCount}}", iErrors.ToString()).Replace("{{WarningCount}}", iWarnings.ToString()).Replace("{{RunTime}}", sRunTime).Replace("{{MachineName}}", sMachineName).Replace("{{ExecutionAccount}}", sExecAcctName).Replace("{{WasInteractive}}", sInteractive).Replace("{{PackageStartTime}}", dtStartTime.ToString()).Replace("{{FrameworkVersion}}", "2.0").Replace("{{PackageEndTime}}", dtEndTime.ToString())

            sTemplate = ReplaceSubStringBetween(sTemplate, "<!--{{AlertBlockStart}}-->", "<!--{{AlertBlockEnd}}-->", sAlertRows, True)
            Return sTemplate
        Else
            oSB.AppendLine("Package: " & sPkgName)
            oSB.AppendLine("Status: " & sStatus)
            If bForSMS Then
                oSB.AppendLine("Start Time: " & dtStartTime.ToString("MM/dd HH:mm:ss"))
            Else
                oSB.AppendLine("Start Time: " & dtStartTime)
            End If
            If bPkgRunning = False Then
                If bForSMS = False Then
                    oSB.AppendLine("End Time: " & dtEndTime)
                End If
                oSB.AppendLine("Run Time: " & FormatTimeDifference(dtStartTime, dtEndTime))
            Else
                If bForSMS = False Then
                    oSB.AppendLine("End Time: <Still Running>")
                End If
            End If
            If bForSMS = False Then
                oSB.AppendLine("Total Errors: " & iErrors)
                oSB.AppendLine("Total Warnings: " & iWarnings)
                oSB.AppendLine("Machine Name: " & sMachineName)
                oSB.AppendLine("Was Interactive?: " & sInteractive)
                oSB.AppendLine("Execution Account: " & sExecAcctName)
                oSB.AppendLine("")
            End If


            Return oSB.ToString() + oSBAlerts.ToString()
        End If
    End Function

    Private Function GetHTMLNotificationMessage(ByVal sPkgName As String, ByVal dtStartTime As DateTime, ByVal dtEndTime As DateTime, ByVal sRunTime As String, ByVal sMachineName As String, ByVal sExecAcctName As String, _
