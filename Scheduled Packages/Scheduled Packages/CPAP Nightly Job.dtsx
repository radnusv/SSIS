                    If String.IsNullOrEmpty(sWarningBlock) = False Then
                        sWarningSourceBlock = GetSubStringBetween(sWarningBlock, "<!--{{AlertSourceStart}}-->", "<!--{{AlertSourceEnd}}-->")
                        'sWarningRow = GetSubStringBetween(sWarningSourceBlock, "<!--{{AlertRowStart}}-->", "<!--{{AlertRowEnd}}-->") 

                        sWarningRow = GetSubStringBetween(sWarningBlock, "<!--{{AlertRowStart}}-->", "<!--{{AlertRowEnd}}-->")
                    End If
                Catch ex As Exception
                    'Dts.FireError "Can not load template file. " & ex.Message 
                    '''/Dont use template becuase we cant load file 
                    bUseCustEmailTemplate = False
                End Try
            End If

            Dim sAlertHTML As String = ""
            '''//////////////////////////////////////// 
            'Loop Through ERRORS 
            '''//////////////////////////////////////// 
            Dim sErrorHTML As String = ""
            Dim sPrevSource As String = ""
            '''/Dim sCurSourceBlock As String = "" 
            Dim sSourceAlertRows As String = ""
            For Each oLoopAlertInfo As Object In oAlertList

                If DirectCast(GetPropVal(oLoopAlertInfo, "AlertType"), enumAlertType) <> enumAlertType.ErrorAlert Then
                    Continue For
                End If
                sAlertType = "ERROR"
                iErrors = iErrors + 1
                sColor = "FFD5D5"

                If bUseCustEmailTemplate = True AndAlso String.IsNullOrEmpty(sErrorRow) = True Then
                    Continue For
                End If

                If sPrevSource <> GetPropVal(oLoopAlertInfo, "SourceName").ToString() Then
                    '''/Alert Source Line 
                    If bUseCustEmailTemplate = True Then
                        If iErrors > 1 Then
                            '''/Prev Source Block 
                            sErrorHTML = ReplaceSubStringBetween(sErrorHTML, "<!--{{AlertRowStart}}-->", "<!--{{AlertRowEnd}}-->", sSourceAlertRows, True)
                        End If
                        If String.IsNullOrEmpty(sWarningSourceBlock) = False Then
                            sErrorHTML = sErrorHTML + sErrorSourceBlock.Replace("{{AlertSourceName}}", GetPropVal(oLoopAlertInfo, "SourceName").ToString())
                        Else
                            sErrorHTML = sErrorHTML + sErrorRow.Replace("{{AlertSourceName}}", GetPropVal(oLoopAlertInfo, "SourceName").ToString())
                        End If

                        sSourceAlertRows = ""
                    Else
                        sErrorHTML = sErrorHTML + (("<tr><td width=""100%"" bgcolor=""#" & sColor & """><b>Source :</b> ") + GetPropVal(oLoopAlertInfo, "SourceName").ToString() & "</td></tr>") + System.Environment.NewLine
                    End If
                End If
                '''/Alert Line 
                If bUseCustEmailTemplate = True Then
                    '''/ 
                    sSourceAlertRows = sSourceAlertRows + sErrorRow.Replace("{{AlertSourceName}}", GetPropVal(oLoopAlertInfo, "SourceName").ToString()).Replace("{{AlertTime}}", GetPropVal(oLoopAlertInfo, "AlertTimeStamp").ToString()).Replace("{{AlertType}}", "Error").Replace("{{AlertMessage}}", GetPropVal(oLoopAlertInfo, "AlertDescription").ToString()).Replace("{{AlertCode}}", GetPropVal(oLoopAlertInfo, "AlertCode").ToString())
                Else
                    sErrorHTML = sErrorHTML + ((("<tr><td width=""100%""><b>" & GetPropVal(oLoopAlertInfo, "AlertTimeStamp").ToString() & " : " & "</b>[") + GetPropVal(oLoopAlertInfo, "AlertCode").ToString() & "] ") + GetPropVal(oLoopAlertInfo, "AlertDescription").ToString() & "</td></tr>") + System.Environment.NewLine
                End If

                sPrevSource = GetPropVal(oLoopAlertInfo, "SourceName").ToString()
            Next

            If iErrors > 0 Then
                If bUseCustEmailTemplate = True Then
                    '''/Replace last source detail after loop is done 
                    '''/Populate Prev Source Block 
                    sErrorHTML = ReplaceSubStringBetween(sErrorHTML, "<!--{{AlertRowStart}}-->", "<!--{{AlertRowEnd}}-->", sSourceAlertRows, True)
                    '''/Populate Error Block 
                    If String.IsNullOrEmpty(sAlertBlock) = False Then
                        sAlertHTML = sAlertHTML + sErrorHTML
                    Else
