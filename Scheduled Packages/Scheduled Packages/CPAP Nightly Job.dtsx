<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{DDCA818A-B331-4A2B-A1C7-68DB5802E027}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource=" &#xA;&#xA;-- truncate table testdb..pps_cpappatdetail&#xA;&#xA;-- &#xA;&#xA;-- exec dbo.PPS_CPAPPrimaryBaseLoader_Test&#xA;&#xA;-- exec dbo.PPS_CPAPSupplyorders_test&#xA;&#xA;-- exec dbo.PPS_CPAPMissingPats_test&#xA;&#xA;-- exec dbo.PPS_CPAPFinalSearch_Test&#xA;&#xA;-- exec dbo.PPS_CPAPLoad404Pats&#xA;&#xA;-- &#xA;&#xA;-- &#xA;&#xA;-- update pps_cpappatdetail set cpcomplstatusdesc = 'Inactive',cpcomplstatus = 6 &#xA;&#xA;-- from pps_cpappatdetail where custnum in &#xA;&#xA;-- (select a.custnum&#xA;&#xA;--  from pps_cpappatdetail a left outer join&#xA;&#xA;-- testdb..pps_cpappatdetail b on a.custnum = b.custnum&#xA;&#xA;-- where &#xA;&#xA;-- b.custnum is null)&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 0: Execution start.',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;UPDATE  PPS_CPAPPatDetail&#xA;SET     address = RTRIM(LTRIM(addr_1_cm)) + '&lt;br&gt; ' + RTRIM(LTRIM(Addr_2_CM))&#xA;        + '&lt;br&gt; ' + RTRIM(LTRIM(City_Cm)) + ', ' + SUBSTRING(zip_cm, 1, 5) ,&#xA;        phone = '(' + SUBSTRING(ph_Num_Cm, 1, 3) + ') ' + SUBSTRING(ph_Num_Cm,&#xA;                                                              4, 3) + '-'&#xA;        + SUBSTRING(ph_Num_Cm, 7, 10) ,&#xA;        Age = DATEDIFF(yy, Birth_Dt_pm, GETDATE()) ,&#xA;        Diagnosis = RTRIM(LTRIM(digmas1.DESCRIPTION1_DG)) ,&#xA;        insnum = i.insnum_is ,&#xA;        insname = SUBSTRING(i.ins_name_is, 1, 30) ,&#xA;        custname = cusmas.name_cm ,&#xA;        physnum = phys_pm ,&#xA;        physname = phy_name_dc ,&#xA;        dob = patmas.birth_dt_pm ,&#xA;        salespersonid = cusmas.Sal001_CM ,&#xA;        salespersonname = s.salespersonname&#xA;&#xA;-- select rtrim(ltrim(addr_1_cm)) + '&lt;br&gt; ' + rtrim(ltrim(Addr_2_CM)) + '&lt;br&gt; ' + rtrim(ltrim(City_Cm)) + ', ' + substring(zip_cm,1,5),&#xA;&#xA;-- ce.Centerid,ce.Centername,&#xA;&#xA;-- '(' + substring(ph_Num_Cm,1,3) + ') ' + substring(ph_Num_Cm,4,3) + ' - ' + substring(ph_Num_Cm,7,10),&#xA;&#xA;-- DATEDIFF(yy, Birth_Dt_pm, getdate()) ,&#xA;&#xA;-- digmas1.DESCRIPTION1_DG,Diagnosis &#xA;FROM    tims.dbo.cusmas cusmas&#xA;        JOIN PPS_CPAPPatDetail AS Patdetail ON cusmas.custnum_cm = patdetail.custnum&#xA;        JOIN tims..patmas patmas ON cusmas.custnum_cm = patmas.custnum_pm&#xA;        LEFT JOIN tims..digmas digmas1 ON patmas.MDiag1_PM = digmas1.Diag1_dg&#xA;        LEFT JOIN tims..docmas docmas ON docmas.physnum_DC = PHYS_PM&#xA;        LEFT JOIN portalprod..Salesperson s ON cusmas.Sal001_CM = s.SalespersonId&#xA;        LEFT JOIN tims.dbo.insmas i ON patmas.InsNum1_PM = i.InsNum_IS&#xA;        LEFT JOIN portalprod..center ce ON cusmas.branch_cm = ce.CenterId&#xA;WHERE   address &lt;&gt; RTRIM(LTRIM(addr_1_cm)) + '&lt;br&gt; ' + RTRIM(LTRIM(Addr_2_CM))&#xA;        + '&lt;br&gt; ' + RTRIM(LTRIM(City_Cm)) + ', ' + SUBSTRING(zip_cm, 1, 5)&#xA;        OR Patdetail.centerid &lt;&gt; ce.Centerid&#xA;        OR Patdetail.centername &lt;&gt; ce.Centername&#xA;        OR PatDetail.phone &lt;&gt; '(' + SUBSTRING(ph_Num_Cm, 1, 3) + ') '&#xA;        + SUBSTRING(ph_Num_Cm, 4, 3) + ' - ' + SUBSTRING(ph_Num_Cm, 7, 10)&#xA;        OR Age &lt;&gt; DATEDIFF(yy, Birth_Dt_pm, GETDATE())&#xA;        OR Diagnosis &lt;&gt; digmas1.DESCRIPTION1_DG&#xA;        OR patdetail.insnum &lt;&gt; i.insnum_is&#xA;        OR patdetail.insname &lt;&gt; i.ins_name_is&#xA;        OR patdetail.custname &lt;&gt; cusmas.name_cm&#xA;        OR patdetail.physnum &lt;&gt; patmas.phys_pm&#xA;        OR patdetail.physname &lt;&gt; docmas.phy_name_dc&#xA;        OR patdetail.dob &lt;&gt; patmas.birth_dt_pm&#xA;        OR patdetail.salespersonid &lt;&gt; cusmas.Sal001_CM&#xA;        OR patdetail.salespersonname &lt;&gt; s.salespersonname&#xA;&#xA;-- &#xA;&#xA;-- drop table testdb..pps_cpappatdetailbckup&#xA;&#xA; &#xA;&#xA;-- &#xA;&#xA;-- select * into testdb..pps_cpappatdetailbckup from pps_cpappatdetail&#xA;&#xA;-- &#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 1: Update PPS_CPAPPatDetails Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;IF EXISTS ( SELECT  *&#xA;            FROM    dbo.sysobjects&#xA;            WHERE   id = OBJECT_ID(N'[Tempaddressupdate]')&#xA;                    AND OBJECTPROPERTY(id, N'IsUserTable') = 1 ) &#xA;    DROP TABLE [Tempaddressupdate]&#xA;&#xA; &#xA;&#xA; &#xA;&#xA; &#xA;&#xA;SELECT  RTRIM(LTRIM(addr_1_cm)) + '&lt;br&gt; ' + RTRIM(LTRIM(Addr_2_CM)) + '&lt;br&gt; '&#xA;        + RTRIM(LTRIM(City_Cm)) + ', ' + RTRIM(LTRIM(p_st_pm)) + ' '&#xA;        + SUBSTRING(zip_cm, 1, 5) address ,&#xA;        custnum&#xA;INTO    Tempaddressupdate&#xA;FROM    pps_cpappatdetail a&#xA;        JOIN tims..patmas b ON a.custnum = b.custnum_pm&#xA;        JOIN tims..cusmas c ON b.custnum_pm = c.custnum_cm&#xA;&#xA;-- &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 2: Tempaddressupdated created',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpappatdetail&#xA;SET     address = b.address&#xA;FROM    pps_cpappatdetail a&#xA;        JOIN Tempaddressupdate b ON a.custnum = b.custnum&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 3: Update PPS_CPAPPatDetails (address) Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA;-- &#xA;&#xA;-- update pps_cpappatdetail set cpcomplstatusdesc = null,cpcomplstatus = null&#xA;&#xA;-- --select * &#xA;&#xA;-- from pps_cpappatdetail where custnum in &#xA;&#xA;-- (select a.custnum&#xA;&#xA;--  from pps_cpappatdetail a left outer join&#xA;&#xA;-- testdb..pps_cpappatdetail b on a.custnum = b.custnum&#xA;&#xA;-- where &#xA;&#xA;-- not b.custnum is null and a.cpcomplstatusdesc = 'Inactive' and a.cpcomplstatus = 6)&#xA;&#xA;-- &#xA;&#xA;-- select * from pps_cpappatdetail&#xA;&#xA;-- insert into pps_cpappatdetail select distinct&#xA;&#xA;-- b.[CustNum]  ,&#xA;&#xA;--    b.[CustName]  ,&#xA;&#xA;--    b.[Phone]  ,&#xA;&#xA;--    b.[CenterId],&#xA;&#xA;--    b.[CenterName],&#xA;&#xA;--    b.[CPComplRepId]  ,&#xA;&#xA;--    b.[CPComplLeadId]  ,&#xA;&#xA;--    b.[CPComplMgrId]  ,&#xA;&#xA;--    b.[PhysNum] ,&#xA;&#xA;--    b.[PhysName] ,&#xA;&#xA;--    b.[SalespersonId]  ,&#xA;&#xA;--    b.[SalespersonName]  ,&#xA;&#xA;--    b.[InsNum] ,&#xA;&#xA;--    b.[InsName] ,&#xA;&#xA;--    b.[DOB]  ,&#xA;&#xA;--    b.[Age] ,&#xA;&#xA;--    b.[Sex]  ,&#xA;&#xA;--    b.[Diagnosis]  ,&#xA;&#xA;--    b.[Address] ,&#xA;&#xA;--    b.[ShipToAddress]  ,&#xA;&#xA;--    b.[ContactPerson] ,&#xA;&#xA;--    b.[Language] ,&#xA;&#xA;--    b.[Status] ,&#xA;&#xA;--    b.[LastCPOrderDate],&#xA;&#xA;--    b.[CPComplFlag] ,&#xA;&#xA;--    b.[CPComplStatus] ,&#xA;&#xA;--    b.[CPComplStatusDesc], &#xA;&#xA;--    b.[CPComplFillStatus],&#xA;&#xA;--    b.[CPComplDueDate], &#xA;&#xA;--    b.[CPComplCallBackDate],&#xA;&#xA;--    b.[CPComplLastUpdDate] ,&#xA;&#xA;--    b.[DueDate] ,&#xA;&#xA;--    b.[Rental] ,&#xA;&#xA;--    null [Flag] ,&#xA;&#xA;--    null [Priority], &#xA;&#xA;--    null [SleepQC] ,&#xA;&#xA;--    null [StartDate]&#xA;&#xA;--  from pps_cpappatdetail a right outer join&#xA;&#xA;-- testdb..pps_cpappatdetail b on a.custnum = b.custnum&#xA;&#xA;-- where a.custnum is null&#xA;&#xA;-- &#xA;&#xA;-- select * from testdb..pps_cpappatdetail&#xA;&#xA;-- &#xA;&#xA;-- /*update PPS_CPAPPatDetail set sleepqc = description from PPS_CPAPPatDetail a &#xA;&#xA;-- join novsqlprod03.ppsapp.dbo.cusmas b  on &#xA;&#xA;-- a.custnum = b.custnum_cm&#xA;&#xA;-- join novsqlprod03.ppsapp.dbo.pps_sleepqcvalues c &#xA;&#xA;-- on b.sleepqc_cm = c.abbreviation */&#xA;&#xA;-- &#xA;&#xA;UPDATE  pps_cpapsupplies&#xA;SET     description = description_IM  &#xA;&#xA;--select * &#xA;FROM    ( SELECT    description_IM ,&#xA;                    partnum_im&#xA;          FROM      sqlprod1.tims.dbo.invmas&#xA;          WHERE     pc_im IN ( 'GS', 'FS', 'BI', 'CA', 'CP' )&#xA;        ) a&#xA;        JOIN pps_cpapsupplies b ON a.partnum_im = b.partnum&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 4: Update PPS_CPAPSupplies Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpapmask&#xA;SET     description = description_IM  &#xA;&#xA;--select * &#xA;FROM    ( SELECT    description_IM ,&#xA;                    partnum_im&#xA;          FROM      sqlprod1.tims.dbo.invmas&#xA;          WHERE     pc_im IN ( 'GS', 'FS', 'BI', 'CA', 'CP' )&#xA;        ) a&#xA;        JOIN pps_cpapmask b ON a.partnum_im = b.partnum&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 5: Update PPS_CPAPMask Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpapheadgear&#xA;SET     description = description_IM  &#xA;&#xA;--select * &#xA;FROM    ( SELECT    description_IM ,&#xA;                    partnum_im&#xA;          FROM      sqlprod1.tims.dbo.invmas&#xA;          WHERE     pc_im IN ( 'GS', 'FS', 'BI', 'CA', 'CP' )&#xA;        ) a&#xA;        JOIN pps_cpapheadgear b ON a.partnum_im = b.partnum&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 6: Update PPS_CPAPHeadgear Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpappatdetail&#xA;SET     rental = NULL &#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpappatdetail&#xA;SET     rental = 1&#xA;FROM    pps_cpappatdetail a&#xA;        JOIN ppsapp..renmas b ON a.custnum = b.custnum_rn&#xA;WHERE   stat_rn = 1&#xA;        AND item_num_rn IN ( '404', '405', '406' )&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 7: Update PPS_CPAPPatDetails (rental) Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;-- update pps_cpappatdetail set priority = null&#xA;&#xA;-- &#xA;&#xA;-- update pps_cpappatdetail set priority = isnull(priority,0) + 1 from pps_cpappatdetail where insnum in ('001','185','186','187')&#xA;&#xA;-- &#xA;&#xA;-- --update pps_cpappatdetail set priority = isnull(priority,0) + 1 where rental = 1&#xA;&#xA;-- &#xA;&#xA;-- update pps_cpappatdetail set priority = isnull(priority,0) + 1 from tims..renmas a join pps_cpappatdetail b on &#xA;&#xA;-- a.custnum_rn = b.custnum where item_num_rn  in ('404','405','406') and ren_rate_rn &lt;&gt; 0&#xA;&#xA;-- &#xA;&#xA;-- &#xA;&#xA;-- update cpaptracking..prescriptionlog set trendprocesseddate = getdate()&#xA;&#xA;-- --select * &#xA;&#xA;-- from cpaptracking..prescriptionlog a join &#xA;&#xA;-- (select * from cpaptracking..cicrec where convert(varchar,cicrecdate,101) = convert(varchar,getdate()-1,101)) b on &#xA;&#xA;-- a.logid = b.logid&#xA;&#xA;-- &#xA;&#xA; &#xA;&#xA;DECLARE @AdvanceDays AS INT &#xA;&#xA;SELECT  @AdvanceDays = ISNULL(settingvalue, 1)&#xA;FROM    dbo.PPS_Settings&#xA;WHERE   SettingName = 'FutureFillDaysToPrintAdvance'&#xA;&#xA;SELECT  a.NoteId ,&#xA;        a.PatNum ,&#xA;        a.CallBackDate ,&#xA;        a.EnteredBy&#xA;INTO    #Temp_FF&#xA;FROM    PPS_CPAPPatNotes a&#xA;        JOIN ( SELECT   patnum ,&#xA;                        MAX(noteid) noteid&#xA;               FROM     pps_CPAPpatnotes&#xA;               GROUP BY patnum&#xA;             ) b ON a.noteid = b.noteid&#xA;WHERE   a.Status = 2&#xA;        AND CAST(a.CallBackDate AS DATETIME) &gt;= CAST(CONVERT(VARCHAR, GETDATE(), 101) AS DATETIME)&#xA;        AND CAST(CONVERT(VARCHAR, a.CallBackDate, 101) AS DATETIME) &lt; CAST(DATEADD(d,&#xA;                                                              @AdvanceDays,&#xA;                                                              CONVERT(VARCHAR, GETDATE(), 101)) AS DATETIME)&#xA;        AND YEAR(a.CallBackDate) = YEAR(GETDATE())&#xA;        AND MONTH(a.CallBackDate) = MONTH(GETDATE())&#xA;GROUP BY a.NoteId ,&#xA;        a.PatNum ,&#xA;        a.EnteredBy ,&#xA;        a.CallBackDate&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 8: #Temp_FF Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;INSERT  INTO PPS_CPAPPatNotes&#xA;        ( patnum ,&#xA;          notes ,&#xA;          equipment ,&#xA;          inactivereason ,&#xA;          ptusage ,&#xA;          usagereason ,&#xA;          status ,&#xA;          callbackdate ,&#xA;          entereddate ,&#xA;          enteredby&#xA;        )&#xA;        SELECT  a.PatNum ,&#xA;                'Converted Future Fill' notes ,&#xA;                0 ,&#xA;                '' ,&#xA;                -1 ,&#xA;                '' ,&#xA;                1 ,&#xA;                DATEADD(m, 1, a.CallBackDate) ,&#xA;                GETDATE() ,&#xA;                a.EnteredBy&#xA;        FROM    PPS_CPAPPatNotes a&#xA;                JOIN ( SELECT   patnum ,&#xA;                                MAX(noteid) noteid&#xA;                       FROM     pps_CPAPpatnotes&#xA;                       GROUP BY patnum&#xA;                     ) b ON a.noteid = b.noteid&#xA;        WHERE   a.Status = 2&#xA;                AND CAST(a.CallBackDate AS DATETIME) &gt;= CAST(CONVERT(VARCHAR, GETDATE(), 101) AS DATETIME)&#xA;                AND CAST(CONVERT(VARCHAR, a.CallBackDate, 101) AS DATETIME) &lt; CAST(DATEADD(d,&#xA;                                                              @AdvanceDays,&#xA;                                                              CONVERT(VARCHAR, GETDATE(), 101)) AS DATETIME)&#xA;                AND YEAR(a.CallBackDate) = YEAR(GETDATE())&#xA;                AND MONTH(a.CallBackDate) = MONTH(GETDATE())&#xA;        GROUP BY a.PatNum ,&#xA;                a.EnteredBy ,&#xA;                a.CallBackDate&#xA;        ORDER BY a.CallBackDate&#xA;&#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 9: Insert into PPS_CPAPPatNotes (for future fills convert to fills) Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA; &#xA;&#xA;/*******************   BACKUP THE TABLES  **/&#xA;&#xA;/*&#xA;&#xA;SELECT * INTO ARPORTALPROD.DBO.PPS_CPAPPATNotes_12012011_TempFF FROM #Temp_FF&#xA;&#xA;SELECT * INTO ARPORTALPROD.DBO.PPS_CPAPGoalsActuals_12012011_1 FROM PPS_CPAPGoalsActuals&#xA;&#xA;SELECT * INTO ARPORTALPROD.DBO.PPS_CPAPPATNotes_12012011_1 FROM PPS_CPAPPatNotes&#xA;&#xA;SELECT * INTO ARPORTALPROD.DBO.pps_cpappatdetail_12012011_1 FROM pps_cpappatdetail&#xA;&#xA;*/&#xA;&#xA; &#xA;&#xA;/*DECLARE FutureFillsCursor CURSOR FOR&#xA;&#xA;select noteid,patnum from PPS_CPAPPatNotes where status = 1 and convert(varchar,entereddate,101) = convert(varchar,getdate(),101)&#xA;&#xA; &#xA;&#xA;declare @Noteid int&#xA;&#xA;declare @Patnum varchar(10)&#xA;&#xA;open FutureFillsCursor&#xA;&#xA;FETCH FutureFillsCursor INTO @Noteid, @Patnum&#xA;&#xA; &#xA;&#xA;while @@fetch_status=0&#xA;&#xA;begin&#xA;&#xA;      exec PPS_CPAPUpdateMetricsActuals @Patnum,@Noteid,0&#xA;&#xA;      exec PPS_CPAPUpdateMetricsActuals2 @Patnum,@Noteid&#xA;&#xA;      FETCH next from FutureFillsCursor INTO @Noteid, @Patnum&#xA;&#xA;end&#xA;&#xA;close FutureFillsCursor&#xA;&#xA;deallocate FutureFillsCursor*/&#xA;&#xA;&#xA;UPDATE  a&#xA;SET     cpapactual = mtdordered + ct ,&#xA;        todayorder = todayordered + ct&#xA;FROM    PPS_CPAPGoalsActuals a&#xA;        JOIN ( SELECT   SUM(cpapactual) mtdordered ,&#xA;                        CenterId ,&#xA;                        COUNT(centerid) ct ,&#xA;                        SUM(todayorder) todayordered&#xA;               FROM     ( SELECT DISTINCT&#xA;                                    cpapactual ,&#xA;                                    c.CenterId ,&#xA;                                    todayorder&#xA;                          FROM      PPSAPP.dbo.vw_CPAPPatDetail a WITH ( NOLOCK )&#xA;                                    JOIN portalprod..center c ON a.CenterId = c.CenterId&#xA;                                    JOIN PPS_CPAPPatNotes d WITH ( NOLOCK ) ON a.custnum = d.patnum&#xA;                                                              AND d.status = 1&#xA;                                                              AND CONVERT(VARCHAR, d.entereddate, 101) = CONVERT(VARCHAR, GETDATE(), 101)&#xA;                                    JOIN PPS_CPAPGoalsActuals e ON c.centerid = e.id&#xA;                                                              AND idtype = 'Center'&#xA;                        ) a&#xA;               GROUP BY CenterId&#xA;             ) b ON a.id = b.centerid&#xA;                    AND a.idtype = 'Center'&#xA;&#xA; &#xA;&#xA;UPDATE  a&#xA;SET     cpapactual = mtdordered ,&#xA;        todayorder = todayordered&#xA;FROM    PPS_CPAPGoalsActuals a&#xA;        JOIN ( SELECT DISTINCT&#xA;                        a.id ,&#xA;                        mtdordered + ct mtdordered ,&#xA;                        todayordered + ct todayordered&#xA;               FROM     PPS_CPAPGoalsActuals a&#xA;                        JOIN ( SELECT   SUM(cpapactual) mtdordered ,&#xA;                                        CPcomplRepId ,&#xA;                                        COUNT(CPcomplRepId) ct ,&#xA;                                        SUM(todayorder) todayordered&#xA;                               FROM     ( SELECT DISTINCT&#xA;                                                    cpapactual ,&#xA;                                                    a.CPcomplRepId ,&#xA;                                                    todayorder&#xA;                                          FROM      PPSAPP.dbo.vw_CPAPPatDetail a&#xA;                                                    WITH ( NOLOCK )&#xA;                                                    JOIN portalprod..center c ON a.CenterId = c.CenterId&#xA;                                                    JOIN PPS_CPAPPatNotes d&#xA;                                                    WITH ( NOLOCK ) ON a.custnum = d.patnum&#xA;                                                              AND d.status = 1&#xA;                                                              AND CONVERT(VARCHAR, d.entereddate, 101) = CONVERT(VARCHAR, GETDATE(), 101)&#xA;                                                    JOIN PPS_CPAPGoalsActuals e ON a.CPcomplRepId = e.id&#xA;                                                              AND idtype = 'CPAP Rep'&#xA;                                        ) a&#xA;                               GROUP BY CPcomplRepId&#xA;                             ) b ON a.id = b.CPcomplRepId&#xA;                                    AND a.idtype = 'CPAP Rep'&#xA;             ) b ON a.id = b.id&#xA;&#xA; &#xA;&#xA;UPDATE  a&#xA;SET     cpapactual = mtdordered ,&#xA;        todayorder = todayordered&#xA;FROM    PPS_CPAPGoalsActuals a&#xA;        JOIN ( SELECT DISTINCT&#xA;                        a.id ,&#xA;                        mtdordered + ct mtdordered ,&#xA;                        todayordered + ct todayordered&#xA;               FROM     PPS_CPAPGoalsActuals a&#xA;                        JOIN ( SELECT   SUM(cpapactual) mtdordered ,&#xA;                                        CPcomplLeadId ,&#xA;                                        COUNT(CPcomplLeadId) ct ,&#xA;                                        SUM(todayorder) todayordered&#xA;                               FROM     ( SELECT DISTINCT&#xA;                                                    cpapactual ,&#xA;                                                    a.CPcomplLeadId ,&#xA;                                                    todayorder&#xA;                                          FROM      PPSAPP.dbo.vw_CPAPPatDetail a&#xA;                                                    WITH ( NOLOCK )&#xA;                                                    JOIN portalprod..center c ON a.CenterId = c.CenterId&#xA;                                                    JOIN PPS_CPAPPatNotes d&#xA;                                                    WITH ( NOLOCK ) ON a.custnum = d.patnum&#xA;                                                              AND d.status = 1&#xA;                                                              AND CONVERT(VARCHAR, d.entereddate, 101) = CONVERT(VARCHAR, GETDATE(), 101)&#xA;                                                    JOIN PPS_CPAPGoalsActuals e ON a.CPcomplLeadId = e.id&#xA;                                                              AND idtype = 'CPAP Lead'&#xA;                                        ) a&#xA;                               GROUP BY CPcomplLeadId&#xA;                             ) b ON a.id = b.CPcomplLeadId&#xA;                                    AND a.idtype = 'CPAP Lead'&#xA;             ) b ON a.id = b.id&#xA;&#xA; &#xA;&#xA;UPDATE  a&#xA;SET     cpapactual = mtdordered ,&#xA;        todayorder = todayordered&#xA;FROM    PPS_CPAPGoalsActuals a&#xA;        JOIN ( SELECT DISTINCT&#xA;                        a.id ,&#xA;                        mtdordered + ct mtdordered ,&#xA;                        todayordered + ct todayordered&#xA;               FROM     PPS_CPAPGoalsActuals a&#xA;                        JOIN ( SELECT   SUM(cpapactual) mtdordered ,&#xA;                                        CPAPMgrId ,&#xA;                                        COUNT(CPAPMgrId) ct ,&#xA;                                        SUM(todayorder) todayordered&#xA;                               FROM     ( SELECT DISTINCT&#xA;                                                    cpapactual ,&#xA;                                                    c.CPAPMgrId ,&#xA;                                                    todayorder&#xA;                                          FROM      PPSAPP.dbo.vw_CPAPPatDetail a&#xA;                                                    WITH ( NOLOCK )&#xA;                                                    JOIN portalprod..center c ON a.CenterId = c.CenterId&#xA;                                                    JOIN PPS_CPAPPatNotes d&#xA;                                                    WITH ( NOLOCK ) ON a.custnum = d.patnum&#xA;                                                              AND d.status = 1&#xA;                                                              AND CONVERT(VARCHAR, d.entereddate, 101) = CONVERT(VARCHAR, GETDATE(), 101)&#xA;                                                    JOIN PPS_CPAPGoalsActuals e ON e.id = CPAPMgrId&#xA;                                                              AND idtype = 'CPAP Mgr'&#xA;                                        ) a&#xA;                               GROUP BY CPAPMgrId&#xA;                             ) b ON a.id = b.CPAPMgrId&#xA;                                    AND a.idtype = 'CPAP Mgr'&#xA;             ) b ON a.id = b.id&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 10: Update CPAP metrics Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;UPDATE  pps_cpappatdetail&#xA;SET     cpcomplstatusdesc = 'Filled' ,&#xA;        cpcomplstatus = 1 ,&#xA;        CPComplFillStatus = 1 ,&#xA;        cpcomplcallbackdate = DATEADD(m, 1, CallBackDate) ,&#xA;        cpcompllastupddate = GETDATE() ,&#xA;        duedate = CONVERT(VARCHAR, DATEADD(m, 1, CallBackDate), 101) ,&#xA;        CPComplFillDate = GETDATE()&#xA;&#xA;--select *&#xA;FROM    pps_cpappatdetail a&#xA;        JOIN ( SELECT DISTINCT&#xA;                        PatNum ,&#xA;                        CallBackDate&#xA;               FROM     #Temp_FF&#xA;             ) b ON a.custnum = b.patnum&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 11: Update PPS_CPAPPatDetails (compliance status) Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;INSERT  INTO pps_cpappatorderinfo&#xA;        ( noteid ,&#xA;          cust_num ,&#xA;          shipname ,&#xA;          shiptype ,&#xA;          addresschange ,&#xA;          onetime ,&#xA;          oktoleave ,&#xA;          ordernotes ,&#xA;          orderdate&#xA;        )&#xA;        SELECT  c.noteid ,&#xA;                cust_num ,&#xA;                shipname ,&#xA;                shiptype ,&#xA;                addresschange ,&#xA;                onetime ,&#xA;                oktoleave ,&#xA;                ordernotes ,&#xA;                a.callbackdate&#xA;        FROM    #Temp_FF a&#xA;                JOIN pps_cpappatorderinfo b ON a.noteid = b.noteid&#xA;                                               AND a.patnum = b.cust_num&#xA;                JOIN ( SELECT   a.patnum ,&#xA;                                a.noteid&#xA;                       FROM     pps_CPAPpatnotes a&#xA;                                JOIN ( SELECT   MAX(noteid) noteid ,&#xA;                                                patnum&#xA;                                       FROM     pps_CPAPpatnotes&#xA;                                       GROUP BY patnum&#xA;                                     ) b ON a.noteid = b.noteid&#xA;                                            AND status = 1&#xA;                     ) c ON b.cust_num = c.patnum&#xA;        GROUP BY c.noteid ,&#xA;                cust_num ,&#xA;                shipname ,&#xA;                shiptype ,&#xA;                addresschange ,&#xA;                onetime ,&#xA;                oktoleave ,&#xA;                ordernotes ,&#xA;                a.callbackdate&#xA;        ORDER BY a.callbackdate &#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 12: Insert into PPS_CPAPPatOrderInfo Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;INSERT  INTO pps_cpappatorders&#xA;        ( cust_num ,&#xA;          noteid ,&#xA;          partnum ,&#xA;          notes ,&#xA;          zero ,&#xA;          qty&#xA;        )&#xA;        SELECT  cust_num ,&#xA;                c.noteid ,&#xA;                partnum ,&#xA;                notes ,&#xA;                zero ,&#xA;                qty&#xA;        FROM    #Temp_FF a&#xA;                JOIN pps_cpappatorders b ON a.noteid = b.noteid&#xA;                                            AND a.patnum = b.cust_num&#xA;                JOIN ( SELECT   a.patnum ,&#xA;                                a.noteid&#xA;                       FROM     pps_CPAPpatnotes a&#xA;                                JOIN ( SELECT   MAX(noteid) noteid ,&#xA;                                                patnum&#xA;                                       FROM     pps_CPAPpatnotes&#xA;                                       GROUP BY patnum&#xA;                                     ) b ON a.noteid = b.noteid&#xA;                                            AND status = 1&#xA;                     ) c ON b.cust_num = c.patnum&#xA;        GROUP BY id ,&#xA;                cust_num ,&#xA;                c.noteid ,&#xA;                partnum ,&#xA;                notes ,&#xA;                zero ,&#xA;                qty&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 13: Insert into PPS_CPAPPatOrders Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;DROP TABLE #Temp_FF&#xA;&#xA;DROP TABLE Tempaddressupdate&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 14: Drop temp tables Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;TRUNCATE TABLE PPS_CPAPPATDETAILYesterday&#xA;&#xA;INSERT  INTO PPS_CPAPPATDETAILYesterday&#xA;        SELECT  *&#xA;        FROM    CPAPDASHBOARD.DBO.PPS_CPAPPATDETAIL&#xA;&#xA; &#xA;&#xA;EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 15: Truncate/Insert PPS_CPAPPatDetail_Yesterday Completed',&#xA;    'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;IF ( DAY(GETDATE()) = 1 ) &#xA;    BEGIN&#xA;&#xA;        DECLARE @str AS VARCHAR(1000)&#xA;&#xA;        SET @str = 'select * into pps_cpappatdetail_'&#xA;            + LEFT(CONVERT(VARCHAR, GETDATE() - 1, 107), 3)&#xA;            + CONVERT(VARCHAR, YEAR(GETDATE() - 1))&#xA;            + ' from pps_cpappatdetail'&#xA;&#xA;        EXEC(@str)&#xA;&#xA;        SET @str = 'select * into pps_cpappatnotes_'&#xA;            + LEFT(CONVERT(VARCHAR, GETDATE() - 1, 107), 3)&#xA;            + CONVERT(VARCHAR, YEAR(GETDATE() - 1)) + ' from pps_cpappatnotes'&#xA;&#xA;        EXEC(@str)&#xA;&#xA; &#xA;&#xA;        EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 16: Monthly archive of PPS_CPAPPatDetails, PPS_CPAPPatNotes Completed',&#xA;            'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA; &#xA;&#xA;        EXEC PortalProd.DBO.NightlyProcessLogs_Insert 'Step 999: Execution end.',&#xA;            'CPAP Nightly Job : Nightly Update of CPAP Data'&#xA;&#xA;    END&#xA;&#xA; &#xA;&#xA; &#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
